 * Defines a weak garbage collector for the vm.
 *
 */

#ifndef GARBAGECOLLECTOR_H_
#define GARBAGECOLLECTOR_H_
// good we can use C++11
#include <memory>

namespace vm {
namespace runtime {

class GarbageCollector {
public:
	GarbageCollector();
	virtual ~GarbageCollector();
};

} /* namespace runtime */
} /* namespace vm */

#endif /* GARBAGECOLLECTOR_H_ */
